<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".fragments.TotalPaidFragment">


    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:scrollbars="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="@dimen/margin_50"
        android:orientation="vertical">

    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_gravity="center_horizontal"
        android:layout_margin="15dp"
        android:orientation="horizontal"
        android:weightSum="2">

        <TextView
            android:id="@+id/dailyEarn_txtId"
            android:layout_width="110dp"
            android:layout_height="45dp"
            android:layout_gravity="center_horizontal"
            android:layout_weight="1"
            android:background="@drawable/color_pink"
            android:clickable="true"
            android:backgroundTint="@color/color_yellow"
            android:contextClickable="true"
            android:focusable="true"
            android:gravity="center_vertical"
            android:text="Daily Earnings"
            android:textAlignment="center"
            android:textColor="@color/color_txtBlue"
            android:textSize="14dp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />


        <TextView
            android:id="@+id/daily_txtId"
            android:layout_width="110dp"
            android:layout_height="45dp"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:background="@drawable/color_gray"
            android:clickable="true"
            android:contextClickable="true"
            android:focusable="true"
            android:gravity="center_vertical"
            android:padding="@dimen/margin_3"
            android:text="Daily"
            android:textAlignment="center"
            android:textColor="@color/colorBlack"
            android:textSize="14dp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />


        <TextView
            android:id="@+id/paymentEstimate_txtId"
            android:layout_width="110dp"
            android:layout_height="45dp"
            android:layout_marginLeft="10dp"
            android:layout_weight="1"
            android:background="@drawable/color_gray"
            android:clickable="true"
            android:contextClickable="true"
            android:focusable="true"
            android:gravity="center_vertical"
            android:padding="@dimen/margin_3"
            android:text="Estimate Payo"
            android:textAlignment="center"
            android:textColor="@color/colorBlack"
            android:textSize="14dp"
            android:textStyle="bold"
            tools:ignore="RtlCompat" />


    </LinearLayout>


        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="\u20B910,000"
            android:gravity="center"
            android:layout_marginTop="@dimen/margin_80"
            android:textColor="@color/color_txtBlue"
            android:textSize="@dimen/textsize_30"
            android:textStyle="bold"/>



        <TextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:text="Total Paid"
            android:gravity="center"
            android:textColor="@color/color_txtBlue"
            android:textSize="@dimen/textsize_20"
            android:textStyle="bold"/>






        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_margin="@dimen/textsize_20"
            android:weightSum="2"
            android:orientation="horizontal">


            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/margin_130"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_weight="1"
                android:layout_marginRight="@dimen/margin_5"
                app:cardBackgroundColor="#de5d5d"
                android:layout_gravity="center"
                app:cardCornerRadius="@dimen/padding_10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/padding_15"
                    android:orientation="vertical">




                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="5263"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_20"
                        android:textStyle="bold"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Tution\nCompleted Today"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_15"
                        android:textStyle="bold"/>






                </LinearLayout>








            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/margin_130"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_marginLeft="@dimen/margin_5"
                android:layout_weight="1"
                android:layout_gravity="center"
                app:cardBackgroundColor="#fb993e"
                app:cardCornerRadius="@dimen/padding_10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/padding_15"
                    android:orientation="vertical">




                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="43346"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_20"
                        android:textStyle="bold"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Tution\nCompleted Today"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_15"
                        android:textStyle="bold"/>






                </LinearLayout>








            </androidx.cardview.widget.CardView>





        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_margin="@dimen/textsize_20"
            android:weightSum="2"
            android:orientation="horizontal">


            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/margin_130"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_weight="1"
                android:layout_marginRight="@dimen/margin_5"
                app:cardBackgroundColor="#817ede"
                android:layout_gravity="center"
                app:cardCornerRadius="@dimen/padding_10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/padding_15"
                    android:orientation="vertical">




                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="5263"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_20"
                        android:textStyle="bold"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Tution Completed\nMonthly"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_15"
                        android:textStyle="bold"/>






                </LinearLayout>








            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/margin_130"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_marginLeft="@dimen/margin_5"
                android:layout_weight="1"
                android:layout_gravity="center"
                app:cardBackgroundColor="#0eafc0"
                app:cardCornerRadius="@dimen/padding_10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/padding_15"
                    android:orientation="vertical">




                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="43346"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_20"
                        android:textStyle="bold"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Fully Completed\nTution"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_15"
                        android:textStyle="bold"/>






                </LinearLayout>








            </androidx.cardview.widget.CardView>





        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:gravity="center"
            android:layout_margin="@dimen/textsize_20"
            android:weightSum="2"
            android:orientation="horizontal">


            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/margin_130"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_weight="1"
                android:layout_marginRight="@dimen/margin_5"
                app:cardBackgroundColor="#2fbf78"
                android:layout_gravity="center"
                app:cardCornerRadius="@dimen/padding_10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/padding_15"
                    android:orientation="vertical">




                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="5263"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_20"
                        android:textStyle="bold"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Completion      \nRate"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_15"
                        android:textStyle="bold"/>






                </LinearLayout>








            </androidx.cardview.widget.CardView>

            <androidx.cardview.widget.CardView
                android:layout_width="@dimen/margin_130"
                android:layout_height="wrap_content"
                android:clickable="true"
                android:layout_marginLeft="@dimen/margin_5"
                android:layout_weight="1"
                android:layout_gravity="center"
                app:cardBackgroundColor="#2fbf78"
                app:cardCornerRadius="@dimen/padding_10">

                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:padding="@dimen/padding_15"
                    android:orientation="vertical">




                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="43346"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_20"
                        android:textStyle="bold"/>


                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Cancellation\n"
                        android:gravity="center"
                        android:textColor="@color/colorWhite"
                        android:textSize="@dimen/textsize_15"
                        android:textStyle="bold"/>






                </LinearLayout>








            </androidx.cardview.widget.CardView>





        </LinearLayout>


    </LinearLayout>

    </androidx.core.widget.NestedScrollView>


</FrameLayout>